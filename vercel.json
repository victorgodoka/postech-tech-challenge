{
  "version": 2,
  "name": "postech-tech-challenge",
  "builds": [
    {
      "src": "apps/dashboard/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/dashboard/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "cd apps/dashboard && npm run build",
  "devCommand": "cd apps/dashboard && npm run dev",
  "installCommand": "npm install && cd apps/dashboard && npm install",
  "outputDirectory": "apps/dashboard/.next"
}
