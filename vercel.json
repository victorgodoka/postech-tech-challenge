{
  "version": 2,
  "name": "postech-tech-challenge",
  "buildCommand": "nx build dashboard",
  "devCommand": "nx serve dashboard",
  "installCommand": "npm install",
  "outputDirectory": "dist/apps/dashboard",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "apps/dashboard/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
