{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "comment": "This is a reference file. Use individual vercel.json files in each app directory for deployment.",
  "projects": {
    "dashboard": {
      "root": "apps/dashboard",
      "framework": "nextjs",
      "buildCommand": "npm run build",
      "outputDirectory": ".next"
    },
    "home": {
      "root": "apps/home",
      "framework": "vite",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    }
  }
}
