{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "useUnknownInCatchVariables": false,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.cjs",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/ConsentCheckbox",
    "eslint.config.js",
    "apps/dashboard/postcss.config.js"
  ],
  "extends": "./tsconfig.base.json"
}
